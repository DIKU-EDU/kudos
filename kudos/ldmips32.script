/*                                            (for emacs: -*- c -*-)
 *  A linker script for KUDOS
 */

ENTRY(_start)
OUTPUT_FORMAT("elf32-bigmips")

SECTIONS
{
    . = 0x80010000;
    .text   : { *(.text) }
    .rodata : { *(.rodata .rodata.*) }
    .data   : { *(.data) }
    .bss    : { *(.bss COMMON) }

     KERNEL_ENDS_HERE = .;
}





